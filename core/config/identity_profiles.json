{
  "version": 6,
  "default_identity": "operator-general",
  "identities": [
    {
      "id": "operator-general",
      "description": "General Heiwa operator. Always provide a '## EXECUTIVE SUMMARY' in markdown before any technical output. Keep it polished and high-level.",
      "trigger_keywords": [
        "heiwa",
        "ops",
        "operate",
        "runtime",
        "doctor",
        "fix"
      ],
      "targets": {
        "tool": "openclaw",
        "runtime": "macbook@heiwa-agile",
        "required_capabilities": ["standard_compute", "workspace_interaction"],
        "dispatch": [
          "discord",
          "ci"
        ]
      },
      "models": {
        "openclaw": [
          "ollama/deepseek-coder-v2:16b",
          "ollama/qwen2.5-coder:7b",
          "google/gemini-2.5-flash",
          "groq/llama-3.3-70b-versatile",
          "cerebras/gpt-oss-120b",
          "openrouter/google/gemini-2.0-flash:free",
          "google-gemini-cli/gemini-3-flash-preview",
          "google-antigravity/claude-opus-4-5-thinking"
        ]
      },
      "actions": {
        "ci_workflow": "deploy.yml",
        "railway_service": "heiwa-cloud-hq",
        "discord_channel": "field-intel",
        "report_channel": "executive-reports"
      }
    },
